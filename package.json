{
  "name": "uploader",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron src/electron/sucrase.js",
    "build:electron": "rollup -c --environment NODE_ENV:production,TARGET:electron",
    "build:frontend": "rollup -c --environment NODE_ENV:production,TARGET:frontend",
    "build": "run-p build:*",
    "watch:frontend": "rollup -cw --environment NODE_ENV:development,API_ENV:production,TARGET:frontend",
    "tape": "sucrase-node node_modules/tape/bin/tape ./tests/**/*.ts | tap-min",
    "tape:watch": "watch 'npm run tape' src tests -du",
    "tsc": "tsc --noEmit",
    "test": "run-p tape tsc",
    "dev": "run-p watch:frontend start",
    "prettier": "prettier --write 'src/**/*.{js,jsx,css,ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Modwatch/Uploader.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Modwatch/Uploader/issues"
  },
  "homepage": "https://github.com/Modwatch/Uploader#readme",
  "dependencies": {
    "@modwatch/core": "^0.1.0",
    "history": "^4.10.1",
    "jwt-decode": "^2.2.0",
    "preact": "^10.0.4",
    "preact-async-route": "^2.2.1",
    "preact-router": "^3.1.0",
    "unistore": "^3.5.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@modwatch/types": "^0.3.0",
    "@surma/rollup-plugin-off-main-thread": "^1.1.1",
    "electron": "^7.0.0",
    "electron-packager": "^14.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-assets": "^5.0.0",
    "postcss-custom-properties": "^9.0.2",
    "postcss-nesting": "^7.0.1",
    "postcss-url": "^8.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.3",
    "rollup": "^1.26.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-delete": "^1.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-sucrase": "^2.1.0",
    "rollup-plugin-terser": "^5.1.2",
